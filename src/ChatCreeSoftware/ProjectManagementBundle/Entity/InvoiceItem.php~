<?php

namespace ChatCreeSoftware\ProjectManagementBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ChatCreeSoftware\ProjectManagementBundle\Entity\InvoiceItem
 */
class InvoiceItem
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var string $itemCode
     */
    private $itemCode;

    /**
     * @var string $itemDescription
     */
    private $itemDescription;

    /**
     * @var float $itemPrice
     */
    private $itemPrice;

    /**
     * @var float $quantity
     */
    private $quantity;

    /**
     * @var float $itemRebate
     */
    private $itemRebate;

    /**
     * @var text $itemRebateText
     */
    private $itemRebateText;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set itemCode
     *
     * @param string $itemCode
     */
    public function setItemCode($itemCode)
    {
        $this->itemCode = $itemCode;
    }

    /**
     * Get itemCode
     *
     * @return string 
     */
    public function getItemCode()
    {
        return $this->itemCode;
    }

    /**
     * Set itemDescription
     *
     * @param string $itemDescription
     */
    public function setItemDescription($itemDescription)
    {
        $this->itemDescription = $itemDescription;
    }

    /**
     * Get itemDescription
     *
     * @return string 
     */
    public function getItemDescription()
    {
        return $this->itemDescription;
    }

    /**
     * Set itemPrice
     *
     * @param float $itemPrice
     */
    public function setItemPrice($itemPrice)
    {
        $this->itemPrice = $itemPrice;
    }

    /**
     * Get itemPrice
     *
     * @return float 
     */
    public function getItemPrice()
    {
        return $this->itemPrice;
    }

    /**
     * Set quantity
     *
     * @param float $quantity
     */
    public function setQuantity($quantity)
    {
        $this->quantity = $quantity;
    }

    /**
     * Get quantity
     *
     * @return float 
     */
    public function getQuantity()
    {
        return $this->quantity;
    }

    /**
     * Set itemRebate
     *
     * @param float $itemRebate
     */
    public function setItemRebate($itemRebate)
    {
        $this->itemRebate = $itemRebate;
    }

    /**
     * Get itemRebate
     *
     * @return float 
     */
    public function getItemRebate()
    {
        return $this->itemRebate;
    }

    /**
     * Set itemRebateText
     *
     * @param text $itemRebateText
     */
    public function setItemRebateText($itemRebateText)
    {
        $this->itemRebateText = $itemRebateText;
    }

    /**
     * Get itemRebateText
     *
     * @return text 
     */
    public function getItemRebateText()
    {
        return $this->itemRebateText;
    }
    /**
     * @var ChatCreeSoftware\ProjectManagementBundle\Entity\Invoice
     */
    private $invoice;


    /**
     * Set invoice
     *
     * @param ChatCreeSoftware\ProjectManagementBundle\Entity\Invoice $invoice
     */
    public function setInvoice(\ChatCreeSoftware\ProjectManagementBundle\Entity\Invoice $invoice)
    {
        $this->invoice = $invoice;
    }

    /**
     * Get invoice
     *
     * @return ChatCreeSoftware\ProjectManagementBundle\Entity\Invoice 
     */
    public function getInvoice()
    {
        return $this->invoice;
    }
    /**
     * @var ChatCreeSoftware\ProjectManagementBundle\Entity\Invoicing
     */
    private $invoicing;


    /**
     * Set invoicing
     *
     * @param ChatCreeSoftware\ProjectManagementBundle\Entity\Invoicing $invoicing
     */
    public function setInvoicing(\ChatCreeSoftware\ProjectManagementBundle\Entity\Invoicing $invoicing)
    {
        $this->invoicing = $invoicing;
    }

    /**
     * Get invoicing
     *
     * @return ChatCreeSoftware\ProjectManagementBundle\Entity\Invoicing 
     */
    public function getInvoicing()
    {
        return $this->invoicing;
    }
}
